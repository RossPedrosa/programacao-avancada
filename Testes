#include <stdio.h>
#include <stdlib.h>

int main(){
  int **x, **y;
  int nl, nc, i, j;
  printf("digite nl: ");
  scanf("%d", &nl);
  printf("digite nc: ");
  scanf("%d", &nc);


  x = (int**)malloc(nl * sizeof (int*));

   for(i=0;i<nl;i++){
    x[i]=malloc(nc*sizeof(int));
  }
  for(i=0;i<nl;i++){
    for(int j=0;j<nc;j++){
    x[i][j] = rand()%100;
    }
}
  for(i=0;i<nl;i++){
    for(int j=0;j<nc;j++){
    printf("%2d ", x[i][j]);
    }
    printf("\n");
}

  y = (int**)malloc(nl * sizeof (int*));

   for(i=0;i<nl;i++){
    y[i]=malloc(nc*sizeof(int));
  }
  for(i=0;i<nl;i++){
    for(int j=0;j<nc;j++){
    y[i][j] = x[i][j];
    }
}
  for(i=0;i<nl;i++){
    memcpy(y[i],x[i],sizeof(nc*sizeof(int)))
}


  for(i=0;i<nl;i++){
    for(int j=0;j<nc;j++){
    printf("%2d ", y[i][j]);
    }
    printf("\n");
}



  for(i=0;i<nl;i++){
   free(x[i]);
   free(y[i]);
  }
  free(x);
  return 0;
}
